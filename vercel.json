{
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "headers": [
    {
      "source": "/rch-official/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/rch-official",
      "destination": "/rch-official/index.html"
    },
    {
      "source": "/((?!rch-official).*)",
      "destination": "/index.html"
    }
  ]
}
